{
  "name": "dro",
  "version": "1.0.0",
  "description": "Dynamic range order",
  "main": "build/main/index.js",
  "scripts": {
    "dev": "nodemon",
    "prod": "ts-node ./src/index.ts >>./out/dro.log 2>&1",
    "n": "ts-node ./src/index.ts --n >>./out/dro.log 2>&1",
    "log": "tail -f -n 1000 ./out/dro.log",
    "balances": "ts-node ./src/index.ts --balances",
    "monitor": "ts-node ./src/index.ts --monitor >>./out/swap_events.csv 2>&1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@uniswap/sdk-core": "^3.0.1",
    "@uniswap/smart-order-router": "^2.5.15",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.0",
    "@uniswap/v3-sdk": "^3.8.2",
    "dotenv": "^10.0.0",
    "ethers": "^5.4.7",
    "moment": "^2.29.1",
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2",
    "tiny-invariant": "^1.2.0",
    "typescript": "^4.4.3",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/node": "^16.10.1",
    "@types/yargs": "^17.0.4",
    "nodemon": "^2.0.13",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1"
  }
}
