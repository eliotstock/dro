{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "dro",
            "type": "node",
            "program": "${workspaceFolder}/dro/src/index.ts",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "tsc: build - dro/tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "cwd": "${workspaceFolder}/dro"
        },
        {
            "name": "analysis",
            "type": "node",
            "program": "${workspaceFolder}/analysis/src/index.ts",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "tsc: build - analysis/tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "cwd": "${workspaceFolder}/analysis"
        }
    ]
}